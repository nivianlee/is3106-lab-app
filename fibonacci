import java.util.*;

public class Fib {

  public static void main(String[] args()) {
    Scanner sc = new Scanner(System.in);
    int num = sc.nextInt();
    int ans = fib(num);
    System.out.println(ans);
  }

  private int fib(int n) {
    if (n == 0 || n == 1) {
      return n;
    } else {
      return fib(n-1) + fib(n-2);
    }
  }
}
